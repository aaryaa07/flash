{
  "name": "flash",
  "description": "Focus search boxes without using a mouse",
  "manifest_version": 2,
  "version": "0.1",
  "browser_action": {
    "default_icon": "icon.png"
  },
  "permissions": ["input", "tabs", "http://*/", "https://*/"],
  "content_scripts": [
    {
      "matches": ["http://*/", "https://*/"],
      "js": ["content.js"]
    }
  ],
  "background": {
    "scripts": ["background.js"]
  }
}
