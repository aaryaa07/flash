{
  "name": "flash",
  "description": "Focus search boxes without using a mouse",
  "manifest_version": 2,
  "version": "0.1",
  "browser_action": {
    "default_icon": "icon.png"
  },
  "permissions": ["input"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"],
      "run_at": "document_start"
    }
  ]
}
